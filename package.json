{
  "name": "tods-competition-factory",
  "description": "Create and mutate TODS compliant tournament objects",
  "keywords": [
    "TODS",
    "tournament",
    "tournaments",
    "competition",
    "tennis"
  ],
  "version": "0.4.14",
  "license": "MIT",
  "private": false,
  "jest": {
    "watchPathIgnorePatterns": [
      "generated"
    ]
  },
  "homepage": "https://github.com/CourtHive/tods-competition-factory",
  "bugs": {
    "url": "https://github.com/CourtHive/tods-competition-factory/issues",
    "email": "support@CourtHive.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CourtHive/tods-competition-factory"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "np": {
    "contents": "."
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "release": "np",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx eslint src",
    "prepare": "tsdx build",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "prepublishOnly": "./addVersion $npm_package_version && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint src"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Charles Allen <charles@CourtHive.com> (CourtHive.com)",
  "module": "dist/tods-competition-factory.esm.js",
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.13",
    "@babel/plugin-proposal-optional-chaining": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@types/jest": "26.0.20",
    "@types/node": "14.14.25",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-jest": "26.6.3",
    "docz": "2.3.1",
    "eslint": "7.19.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "highlight.js": "10.6.0",
    "husky": "5.0.9",
    "jest-environment-jsdom": "26.6.2",
    "np": "7.3.0",
    "pinst": "2.1.4",
    "prettier": "2.2.1",
    "prettier-eslint": "12.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "source-map-resolve": "0.6.0",
    "tsdx": "0.14.1",
    "tslib": "2.1.0",
    "typescript": "4.1.3"
  },
  "dependencies": {
    "tods-matchup-format-code": "^1.0.7"
  },
  "resolutions": {
    "axios": "^0.21.1",
    "marked": "^0.7.0 "
  }
}

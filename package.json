{
  "name": "tods-competition-factory",
  "description": "Create and mutate TODS compliant tournament objects",
  "keywords": [
    "TODS",
    "tournament",
    "advanced-scheduling",
    "tournament-algorithm",
    "tournament-draw-generator",
    "tournament-manager",
    "tournaments",
    "competition",
    "tennis-score",
    "tennis"
  ],
  "version": "0.24.0",
  "license": "MIT",
  "private": false,
  "jest": {
    "coverageDirectory": "./benchmarks",
    "coveragePathIgnorePatterns": [
      "documentation",
      "benchmarks",
      "harness",
      "index.ts",
      "dist"
    ],
    "watchPathIgnorePatterns": [
      "generated"
    ],
    "testPathIgnorePatterns": [
      "benchmarks"
    ]
  },
  "homepage": "https://github.com/CourtHive/tods-competition-factory",
  "bugs": {
    "url": "https://github.com/CourtHive/tods-competition-factory/issues",
    "email": "support@CourtHive.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CourtHive/tods-competition-factory"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "np": {
    "contents": "."
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "release": "np",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "jest --maxWorkers=50%",
    "benchmark": "jest --runInBand benchmark",
    "benchmark-coverage": "jest --runInBand benchmark --collect-coverage",
    "coverage": "jest --maxWorkers=50% --collect-coverage",
    "lint": "eslint src",
    "prepare": "tsdx build",
    "prepublishOnly": "./addVersion $npm_package_version && pinst --disable",
    "postinstall": "husky install",
    "postpublish": "pinst --enable",
    "doc:publish": "cd documentation && GIT_USER=CourtHive USE_SSH=true yarn deploy"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn eslint src"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Charles Allen <charles@CourtHive.com> (CourtHive.com)",
  "module": "dist/tods-competition-factory.esm.js",
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.5",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "@types/jest": "27.0.1",
    "@types/node": "14.17.16",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "babel-jest": "27.2.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "24.4.2",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.25.1",
    "husky": "7.0.2",
    "jest-cli": "27.2.0",
    "jest-environment-node": "27.2.0",
    "np": "7.5.0",
    "pinst": "2.1.6",
    "prettier": "2.4.1",
    "prettier-eslint": "13.0.0",
    "source-map-resolve": "0.6.0",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typescript": "4.4.3"
  },
  "dependencies": {
    "tods-matchup-format-code": "1.1.5"
  },
  "resolutions": {
    "marked": "^3.0.0"
  }
}
